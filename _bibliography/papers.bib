@article{ WOS:000321956900013,
Author = {Mill, Jonathan and Heijmans, Bastiaan T.},
Title = {From promises to practical strategies in epigenetic epidemiology},
Journal = {NATURE REVIEWS GENETICS},
Year = {2013},
Volume = {14},
Number = {8},
Pages = {585-594},
Month = {AUG},
Abstract = {The epigenome has been heralded as a key `missing piece' of the
   aetiological puzzle for complex phenotypes across the biomedical
   sciences. The standard research approaches developed for genetic
   epidemiology, however, are not necessarily appropriate for epigenetic
   studies of common disease. Here, we discuss the optimal execution of
   population-based studies of epigenetic variation, which will contribute
   to the emerging field of `epigenetic epidemiology' and emphasize the
   importance of establishing a causal role in pathology for
   disease-associated epigenetic changes. We propose that improved
   understanding of the molecular mechanisms underlying human health and
   disease are best achieved through carrying out studies of epigenetics in
   populations as a part of an integrated functional genomics strategy.},
DOI = {10.1038/nrg3405},
ISSN = {1471-0056},
EISSN = {1471-0064},
ResearcherID-Numbers = {Mill, Jonathan S/B-3276-2010
   Mill, Jonathan/B-5678-2014},
