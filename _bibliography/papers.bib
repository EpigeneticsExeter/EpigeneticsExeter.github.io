@article{ WOS:000455193900007,
Author = {Smith, Adam R. and Smith, Rebecca G. and Burrage, Joe and Troakes,
   Claire and Al-Sarraj, Safa and Kalaria, Rajesh N. and Sloan, Carolyn and
   Robinson, Andrew C. and Mill, Jonathan and Lunnon, Katie},
Title = {A cross-brain regions study of ANK1 DNA methylation in different
   neurodegenerative diseases},
Journal = {NEUROBIOLOGY OF AGING},
Year = {2019},
Volume = {74},
Pages = {70-76},
Month = {FEB},
Abstract = {Recent epigenome-wide association studies in Alzheimer's disease have
   highlighted consistent robust neuropathology-associated DNA
   hypermethylation of the ankyrin 1 (ANK1) gene in the cortex. The extent
   to which altered ANK1 DNA methylation is also associated with other
   neurodegenerative diseases is not currently known. In the present study,
   we used bisulfite pyrosequencing to quantify DNA methylation across 8
   CpG sites within a 118 bp region of the ANK1 gene across multiple brain
   regions in Alzheimer's disease, Vascular dementia, Dementia with Lewy
   bodies, Huntington's disease, and Parkinson's disease. We demonstrate
   disease-associated ANK1 hypermethylation in the entorhinal cortex in
   Alzheimer's disease, Huntington's disease, and Parkinson's disease,
   whereas in donors with Vascular dementia and Dementia with Lewy bodies,
   we observed elevated ANK1 DNA methylation only in individuals with
   coexisting Alzheimer's disease pathology. We did not observe any
   disease-associated differential ANK1 DNA methylation in the striatum in
   Huntington's disease or the substantia nigra in Parkinson's disease. Our
   data suggest that ANK1 is characterized by region and disease-specific
   differential DNA methylation in multiple neurodegenerative diseases. (C)
   2018 Elsevier Inc. All rights reserved.},
DOI = {10.1016/j.neurobiolaging.2018.09.024},
ISSN = {0197-4580},
