@article{ WOS:000379373900009,
Author = {Devall, Matthew and Roubroeks, Janou and Mill, Jonathan and Weedon,
   Michael and Lunnon, Katie},
Title = {Epigenetic regulation of mitochondrial function in neurodegenerative
   disease: New insights from advances in genomic technologies},
Journal = {NEUROSCIENCE LETTERS},
Year = {2016},
Volume = {625},
Pages = {47-55},
Month = {JUN 20},
Abstract = {The field of mitochondrial epigenetics has received increased attention
   in recent years and changes in mitochondrial DNA (mtDNA) methylation has
   been implicated in a number of diseases, including neurodegenerative
   diseases such as amyotrophic lateral sclerosis. However, current
   publications have been limited by the use of global or targeted methods
   of measuring DNA methylation. In this review, we discuss current
   findings in mitochondrial epigenetics as well as its potential role as a
   regulator of mitochondria within the brain. Finally, we summarize the
   current technologies best suited to capturing mtDNA methylation, and how
   a move towards whole epigenome sequencing of mtDNA may help to advance
   our current understanding of the field. (C) 2016 Elsevier Ireland Ltd.
   All rights reserved.},
DOI = {10.1016/j.neulet.2016.02.013},
ISSN = {0304-3940},
EISSN = {1872-7972},
ResearcherID-Numbers = {Devall, Matthew/GPG-1785-2022
